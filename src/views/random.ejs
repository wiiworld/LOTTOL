<link rel="stylesheet" media="all" href="/css/random.css"><head>    <!-- Global site tag (gtag.js) - Google Analytics -->    <script async src="https://www.googletagmanager.com/gtag/js?id=UA-179612539-1"></script>    <script>        window.dataLayer = window.dataLayer || [];        function gtag(){dataLayer.push(arguments);}        gtag('js', new Date());        gtag('config', 'UA-179612539-1');    </script></head><body><div class="win_result">    <button onclick="window.location.reload(true)">다시추첨</button>    <h4>random number</h4>    <div class="nums">        <div class="num win">            <strong>당첨예상번호</strong>            <p>            </p>        </div>        <div class="num bonus">            <strong>보너스 예상번호</strong>            <p>            </p>        </div>    </div></div></body><script>    var list = []    var numlist = []    var bonus = []    var flag = 1;    while (flag) {        var temp = Math.floor(Math.random() * 44) + 1        console.log(temp)        if (!list.includes(temp)) {            list.push(temp)        }        if (list.length == 7) {            for(var i=0;i<list.length-1;i++){                numlist[i] = list[i]            }            bonus[0] = list[6]            numlist.sort(function (a, b) {                return a - b;            })            flag = 0        }    }    console.log(list)    var windiv = document.getElementsByClassName("num win")    var winp = windiv.item(0).children.item(1)    var bonusdiv = document.getElementsByClassName("num bonus")    var bonusp = bonusdiv.item(0).children.item(1)    for (var i = 0; i < numlist.length; i++) {        var span = document.createElement("span")        var data = document.createTextNode(numlist[i])        span.appendChild(data)        span.classList.add("ball")        span.classList.add("lrg")        if (numlist[i] < 10) {            span.classList.add("ball0")        } else if (numlist[i] < 20) {            span.classList.add("ball1")        } else if (numlist[i] < 30) {            span.classList.add("ball2")        } else if (numlist[i] < 40) {            span.classList.add("ball3")        } else {            span.classList.add("ball4")        }        winp.appendChild(span)    }    //bonus ball    var span = document.createElement("span")    var bonusnum = bonus[0]    var data = document.createTextNode(bonusnum)    span.appendChild(data)    span.classList.add("ball")    span.classList.add("lrg")    if (bonusnum < 10) {        span.classList.add("ball0")    } else if (bonusnum< 20) {        span.classList.add("ball1")    } else if (bonusnum< 30) {        span.classList.add("ball2")    } else if (bonusnum< 40) {        span.classList.add("ball3")    } else {        span.classList.add("ball4")    }    bonusp.appendChild(span)</script><script type="text/css"></script>