<link rel="stylesheet" media="all" href="/css/random.css"><body><p id="space"></p></body><script>    var list = []    var flag = 1;    while(flag){        var temp = Math.floor(Math.random()*45)        console.log(temp)        if (!list.includes(temp)){            list.push(temp)        }        if(list.length == 7){            list.sort(function (a,b){                return a-b;            })            flag = 0        }    }    console.log(list)    var ptag = document.getElementById("space")    for(var i=0;i<list.length;i++){        var span = document.createElement("span")        var data = document.createTextNode(list[i])        span.appendChild(data)        span.classList.add("ball")        span.classList.add("lrg")        if (list[i] < 10) {span.classList.add("ball0")} else        if (list[i] < 20) {span.classList.add("ball1") } else        if (list[i] < 30) {span.classList.add("ball2") } else        if (list[i] < 40) {span.classList.add("ball3") } else                          {span.classList.add("ball4") }        ptag.appendChild(span)    }</script><script type="text/css"></script>