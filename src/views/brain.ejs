<script src="//unpkg.com/brain.js"></script><script src="//code.jquery.com/jquery-3.3.1.min.js"></script><link rel="stylesheet" media="all" href="/css/slider.css"><body><h1> Slider material</h1><div class="slidecontainer">    <p>binaryThresh</p>    <input type="range" min="0.1" max="1" step="0.1" value="0.5" class="slider" id="binaryslider">    <h2 id="binaryThresh"></h2>    <p>HIDDEN LAYER</p>    <input type="range" min="1" max="10" value="5" class="slider" id="myRange">    <h2 id="hiddenlayer"></h2></div><button onclick="train()">Train</button><br><br><h2>result : <h3 id="result"></h3> </h2><script type="application/javascript">    var binaryslider = document.getElementById("binaryslider")    var binaryoutput = document.getElementById("binaryThresh")    var hiddenSlider = document.getElementById("myRange")    var hiddenOutput = document.getElementById("hiddenlayer")    binaryslider.oninput = function(){        binaryoutput.innerHTML = binaryslider.value    }    binaryoutput.innerHTML = binaryslider.value    hiddenSlider.oninput = function() {        hiddenOutput.innerHTML = this.value    }    hiddenOutput.innerHTML = hiddenSlider.value    function train(evt) {        var hiddenLayer = document.getElementById("hiddenlayer")        console.log(evt)        const config = {            binaryThresh: parseFloat(binaryoutput.innerText),            hiddenLayers: [hiddenLayer.innerText],            activation: 'sigmoid',            leakyReluAlpha: 0.01,        };        console.log(config.binaryThresh)        console.log(config.hiddenLayers)        const rnnNet = new brain.RNN(config)        const net = new brain.NeuralNetwork(config);        //Training is computationally expensive, so should try to train the network        //offline or on a Worker and use the toFunction() or toJSON()        net.train([            { input: [0, 0], output: [0] },            { input: [0, 1], output: [1] },            { input: [1, 0], output: [1] },            { input: [1, 1], output: [0] },        ])        //TODO : 1등에 대한 학습, 2등에 대한 학습 진행        const output = net.run([1,0]);        var result = document.getElementById("result")        result.innerText = output[0]    }</script></body>